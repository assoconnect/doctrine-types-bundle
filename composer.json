{
  "name": "assoconnect/doctrine-validator-bundle",
  "description": "Custom Symfony validator for Doctrine entities.",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["Doctrine", "Validation"],
  "authors": [
    {
      "name": "AssoConnect",
      "homepage": "https://www.assoconnect.com"
    },
    {
      "name": "Sylvain Fabre"
    },
    {
      "name": "Julien Waguet"
    }
  ],
  "autoload": {
    "psr-4": {
      "AssoConnect\\DoctrineValidatorBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AssoConnect\\DoctrineValidatorBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "assoconnect/validator-bundle": "^1.0",
    "austinsmorris/doctrine-dbal-datetimeutc": "^2.0",
    "moneyphp/money": "^v3",
    "doctrine/dbal": "^2.8",
    "doctrine/doctrine-bundle": "^1.9|^2.0",
    "php": "^7.1",
    "ramsey/uuid-doctrine": "^1.4",
    "symfony/validator": "^4.0|^5.0",
    "symfony/property-access": "^4.3|^5.0",
    "symfony/intl": "^4.3|^5.0",
    "assoconnect/php-date-bundle": "^2.0"
  },
  "require-dev": {
    "doctrine/cache": "~1.0",
    "doctrine/annotations": "~1.0",
    "phpunit/phpunit": "^7.0",
    "symfony/framework-bundle": "^4.3|^5.0",
    "symfony/yaml": "^4.3|^5.0"
  }
}
